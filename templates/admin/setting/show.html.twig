                <th>Instagram</th>
                <td>{{ setting.instagram }}</td>
            </tr>
            <tr>
                <th>Twitter</th>
                <td>{{ setting.twitter }}</td>
            </tr>
            <tr>
                <th>Youtube</th>
                <td>{{ setting.youtube }}</td>
            </tr>
            <tr>
                <th>Tiktok</th>
                <td>{{ setting.tiktok }}</td>
            </tr>
            <tr>
                <th>Aboutus</th>
                <td>{{ setting.aboutus }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ setting.contact }}</td>
            </tr>
            <tr>
                <th>Reference</th>
                <td>{{ setting.reference }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ setting.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_setting_index') }}">back to list</a>

    <a href="{{ path('app_admin_setting_edit', {'id': setting.id}) }}">edit</a>

    {{ include('admin/setting/_delete_form.html.twig') }}
</div>



{% endblock %}

